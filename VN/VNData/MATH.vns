Func abs (@x) {
	if (@x < 0)
		return -@x
	return @x
}

Func max (@x, @y) {
	if (@x > @y)
		return @x
	return @y
}

Func min (@x, @y) {
	if (@x < @y)
		return @x
	return @y
}

Func floor (@x) {
	return @x >> 0
}

Func ceil (@x) {
	@p = @x
	@x = floor(@x)
	if (@p != @x)
		return @x + 1
	return @x
}

Func round (@x) {
	return floor(@x + 0.5)
}
